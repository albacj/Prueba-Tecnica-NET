@model List<ProcesoShneider.Models.WaterMeter>

<h4>Water Meter</h4>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Serial Number
        </th>
        <th>
            Brand
        </th>
        <th>
            Model
        </th>
        <th></th>
    </tr>

@foreach (ProcesoShneider.Models.WaterMeter item in Model){
    <tr>
        <td>
            @(item.Serial_Number)
        </td>
        <td>
            @(item.Brand)
        </td>
        <td>
            @(item.Model)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "WaterMeterDetails", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>

@model List<ProcesoShneider.Models.Gateway>

<h4>Gateway</h4>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Serial Number
        </th>
        <th>
            Brand
        </th>
        <th>
            Model
        </th>
        <th>
            IP
        </th>
        <th>
            Port
        </th>
        <th></th>
    </tr>

@foreach (ProcesoShneider.Models.Gateway item in Model) {
    <tr>
        <td>
            @(item.SerialNumber)
        </td>
        <td>
            @(item.Brand)
        </td>
        <td>
            @(item.Model)
        </td>
        <td>
            @(item.Ip)
        </td>
        <td>
            @(item.Port)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
